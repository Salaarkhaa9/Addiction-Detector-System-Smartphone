<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.salarsapplication.app.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnaddicto"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_8pxh"
        android:layout_marginTop="@dimen/_3pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_500">
        <androidx.appcompat.widget.Toolbar android:gravity="end"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_59pxh"
            android:layout_marginStart="@dimen/_59pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowaddicto"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyle">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtADDICTO"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.86"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtADDICTO}"
                    style="@style/txtManjariregular40" tools:text="@string/lbl_addicto"/>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_52pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSignUp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameStackellipsesix"
                    android:layout_height="@dimen/_748pxv" android:layout_width="match_parent">
                    <LinearLayout android:id="@+id/linearRowellipsesix"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_98pxv"
                        android:layout_marginEnd="@dimen/_44pxh"
                        android:layout_marginStart="@dimen/_44pxh"
                        android:layout_marginTop="@dimen/_98pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <View
                            android:background="@drawable/rectangle_bg_gray_600_radius_52"
                            android:id="@+id/viewEllipseSix"
                            android:layout_height="@dimen/_104pxh"
                            android:layout_marginTop="@dimen/_32pxv" android:layout_width="@dimen/_104pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_600_radius_17"
                            android:id="@+id/viewEllipseSeven"
                            android:layout_height="@dimen/_34pxh"
                            android:layout_marginBottom="@dimen/_102pxv"
                            android:layout_marginStart="@dimen/_121pxh" android:layout_width="@dimen/_34pxh"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageEllipseEight"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginBottom="@dimen/_73pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_73pxv"
                        android:layout_width="@dimen/_135pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_ellipse8_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse8_1"/>
                    <View
                        android:background="@drawable/rectangle_bg_gray_600_radius_37_5"
                        android:id="@+id/viewEllipseNine"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_75pxh"
                        android:layout_marginBottom="@dimen/_166pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_166pxv" android:layout_width="@dimen/_75pxh"/>
                    <View
                        android:background="@drawable/rectangle_bg_gray_600_radius_48"
                        android:id="@+id/viewEllipseTen"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_96pxh"
                        android:layout_marginEnd="@dimen/_61pxh"
                        android:layout_marginStart="@dimen/_61pxh"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_96pxh"/>
                    <ImageView android:id="@+id/imageEllipseFour"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_80pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_ellipse4_1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse4_1"/>
                    <LinearLayout android:id="@+id/linearColumnsignup"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_63pxv"
                        android:layout_marginEnd="@dimen/_42pxh"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_marginTop="@dimen/_63pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSignUp"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_75pxh"
                            android:layout_marginStart="@dimen/_82pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{signUpVM.signUpModel.txtSignUp}"
                            style="@style/txt" tools:text="@string/lbl_sign_up"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowvectorOne"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_73pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylegray_300cornerRadius">
                            <FrameLayout
                                android:id="@+id/frameCarbonuserfil"
                                android:layout_height="@dimen/_16pxv"
                                android:layout_marginBottom="@dimen/_16pxv"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_16pxv"
                                android:layout_width="@dimen/_16pxh" style="@style/groupStylegray_300">
                                <ImageView
                                    android:id="@+id/imageVectorOne"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_14pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginEnd="@dimen/_3pxh"
                                    android:layout_marginStart="@dimen/_3pxh"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="@dimen/_10pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_vector_gray_600"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_600"/>
                            </FrameLayout>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtUsername"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_12pxv"
                                android:layout_marginEnd="@dimen/_111pxh"
                                android:layout_marginStart="@dimen/_36pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_weight="0.85"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{signUpVM.signUpModel.txtUsername}"
                                style="@style/txtManjariregular20" tools:text="@string/lbl_username"/>
                        </LinearLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowvectorTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_38pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylegray_300cornerRadius">
                            <ImageView android:id="@+id/imageVectorTwo"
                                android:layout_height="@dimen/_9pxh"
                                android:layout_marginBottom="@dimen/_20pxv"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_18pxv"
                                android:layout_width="@dimen/_12pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_gray_600_9x12"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_600_9x12"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtEmailAddress"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_12pxv"
                                android:layout_marginEnd="@dimen/_74pxh"
                                android:layout_marginStart="@dimen/_36pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_weight="0.91"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{signUpVM.signUpModel.txtEmailAddress}"
                                style="@style/txtManjariregular20" tools:text="@string/lbl_email_address"/>
                        </LinearLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowvectorThree"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_38pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylegray_1">
                            <ImageView
                                android:id="@+id/imageVectorThree"
                                android:layout_height="@dimen/_13pxh"
                                android:layout_marginBottom="@dimen/_17pxv"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:layout_width="@dimen/_11pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_gray_600_13x11"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_600_13x11"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtPassword"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_13pxv"
                                android:layout_marginEnd="@dimen/_115pxh"
                                android:layout_marginStart="@dimen/_37pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_weight="0.88"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{signUpVM.signUpModel.txtPassword}"
                                style="@style/txtManjariregular20" tools:text="@string/lbl_password"/>
                        </LinearLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowvectorFour"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_40pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylegray_1">
                            <ImageView android:id="@+id/imageVectorFour"
                                android:layout_height="@dimen/_13pxh"
                                android:layout_marginBottom="@dimen/_17pxv"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:layout_width="@dimen/_11pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_gray_600_13x11"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_600_13x11"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtConfirmPasswor"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_13pxv"
                                android:layout_marginEnd="@dimen/_39pxh"
                                android:layout_marginStart="@dimen/_37pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_weight="0.93"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{signUpVM.signUpModel.txtConfirmPasswor}"
                                style="@style/txtManjariregular20" tools:text="@string/msg_confirm_passwor"/>
                        </LinearLayout>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnSignUpOne"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_70pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_13pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_13pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_sign_up"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_sign_up"/>
                    </LinearLayout>
                </FrameLayout>
                <View
                    android:background="@drawable/rectangle_bg_gray_600_radius_17_5"
                    android:id="@+id/viewEllipseEleven"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_35pxh"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
