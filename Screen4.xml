<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="accountCreatedVM" type="com.salarsapplication.app.modules.accountcreated.data.viewmodel.AccountCreatedVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearColumn"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_300">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnaddicto"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_500">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginEnd="@dimen/_70pxh"
                android:layout_marginStart="@dimen/_81pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowaddicto"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyle">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtADDICTO"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{accountCreatedVM.accountCreatedModel.txtADDICTO}"
                        style="@style/txtManjariregular40" tools:text="@string/lbl_addicto"/>
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_46pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnellipsefour"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageEllipseFour"
                    android:layout_height="@dimen/_80pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_ellipse4_2"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse4_2"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_400_radius_17"
                    android:id="@+id/viewEllipseSeven"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_34pxh"
                    android:layout_marginEnd="@dimen/_44pxh"
                    android:layout_marginStart="@dimen/_44pxh"
                    android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_34pxh"/>
                <FrameLayout android:id="@+id/frameStackellipsesix"
                    android:layout_height="@dimen/_338pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_319pxh">
                    <View
                        android:background="@drawable/rectangle_bg_gray_600_71_radius_52"
                        android:id="@+id/viewEllipseSix"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_104pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_104pxh"/>
                    <LinearLayout android:id="@+id/linearColumnsuccess"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSuccess"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{accountCreatedVM.accountCreatedModel.txtSuccess}"
                            style="@style/txt" tools:text="@string/lbl_success"/>
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtYourAccountha"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{accountCreatedVM.accountCreatedModel.txtYourAccountha}"
                            style="@style/txt_1" tools:text="@string/msg_your_account_ha"/>
                        <ImageView android:id="@+id/imageSearch"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_100pxh"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_34pxv"
                            android:layout_width="@dimen/_100pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_search"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnContinue"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_4pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_69pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_13pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_13pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_continue"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_continue"/>
                    </LinearLayout>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearRowellipseeight"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnellipseeight"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_weight="0.55"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageEllipseEight"
                            android:layout_height="@dimen/_155pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_135pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_ellipse8_2}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse8_2"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_600_radius_17_5"
                            android:id="@+id/viewEllipseEleven"
                            android:layout_height="@dimen/_35pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_113pxv" android:layout_width="@dimen/_35pxh"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnellipsenine"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_75pxv"
                        android:layout_weight="0.45"
                        android:layout_width="0dp" android:orientation="vertical">
                        <View
                            android:background="@drawable/rectangle_bg_gray_400_radius_37_5"
                            android:id="@+id/viewEllipseNine"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_75pxh"
                            android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_75pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_400_radius_48"
                            android:id="@+id/viewEllipseTen"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_96pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_70pxv" android:layout_width="@dimen/_96pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
