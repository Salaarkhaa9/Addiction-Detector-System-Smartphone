<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginVM" type="com.salarsapplication.app.modules.login.data.viewmodel.LoginVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnaddicto"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_8pxh"
        android:layout_marginTop="@dimen/_3pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_500">
        <androidx.appcompat.widget.Toolbar android:gravity="end"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_62pxh"
            android:layout_marginStart="@dimen/_62pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowaddicto"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyle">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtADDICTO"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.86"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtADDICTO}"
                    style="@style/txtManjariregular40" tools:text="@string/lbl_addicto"/>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_52pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnellipsefour"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowellipsefour"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageEllipseFour"
                        android:layout_height="@dimen/_80pxh"
                        android:layout_marginBottom="@dimen/_400pxv"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_ellipse4_80x40"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse4_80x40"/>
                    <FrameLayout android:id="@+id/frameStackellipsesix"
                        android:layout_height="@dimen/_382pxv"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_98pxv" android:layout_width="@dimen/_272pxh">
                        <View
                            android:background="@drawable/rectangle_bg_gray_600_radius_52"
                            android:id="@+id/viewEllipseSix"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_104pxh"
                            android:layout_marginBottom="@dimen/_32pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_32pxv" android:layout_width="@dimen/_104pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_600_radius_17"
                            android:id="@+id/viewEllipseSeven"
                            android:layout_gravity="top|end"
                            android:layout_height="@dimen/_34pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_13pxh"
                            android:layout_marginStart="@dimen/_13pxh" android:layout_width="@dimen/_34pxh"/>
                        <LinearLayout
                            android:id="@+id/linearColumnlogin"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLogin"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_89pxh"
                                android:layout_marginStart="@dimen/_89pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{loginVM.loginModel.txtLogin}"
                                style="@style/txt" tools:text="@string/lbl_login"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearRowvectorOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_62pxv"
                                android:layout_width="match_parent"
                                android:orientation="horizontal" style="@style/groupStylebluegray_100cornerRadius">
                                <ImageView
                                    android:id="@+id/imageVectorOne"
                                    android:layout_height="@dimen/_14pxh"
                                    android:layout_marginBottom="@dimen/_17pxv"
                                    android:layout_marginStart="@dimen/_14pxh"
                                    android:layout_marginTop="@dimen/_17pxv"
                                    android:layout_width="@dimen/_10pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_vector_gray_600"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_600"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtUsername"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_12pxv"
                                    android:layout_marginEnd="@dimen/_111pxh"
                                    android:layout_marginStart="@dimen/_39pxh"
                                    android:layout_marginTop="@dimen/_13pxv"
                                    android:layout_weight="0.9"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{loginVM.loginModel.txtUsername}"
                                    style="@style/txtManjariregular20" tools:text="@string/lbl_username"/>
                            </LinearLayout>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearRowvectorTwo"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_1pxh"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_39pxv"
                                android:layout_width="match_parent"
                                android:orientation="horizontal" style="@style/groupStylebluegray_1">
                                <ImageView
                                    android:id="@+id/imageVectorTwo"
                                    android:layout_height="@dimen/_13pxh"
                                    android:layout_marginBottom="@dimen/_17pxv"
                                    android:layout_marginStart="@dimen/_12pxh"
                                    android:layout_marginTop="@dimen/_17pxv"
                                    android:layout_width="@dimen/_11pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_vector_gray_600_13x11"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_600_13x11"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtPassword"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_13pxv"
                                    android:layout_marginEnd="@dimen/_113pxh"
                                    android:layout_marginStart="@dimen/_41pxh"
                                    android:layout_marginTop="@dimen/_12pxv"
                                    android:layout_weight="0.88"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{loginVM.loginModel.txtPassword}"
                                    style="@style/txtManjariregular20" tools:text="@string/lbl_password"/>
                            </LinearLayout>
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnLogInOne"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_40pxv"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_13pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_13pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_log_in2"
                                android:textAllCaps="false"
                                style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_log_in2"/>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowellipseeight"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnellipseeight"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_weight="0.55"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageEllipseEight"
                            android:layout_height="@dimen/_155pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_135pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_ellipse8_155x135}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse8_155x135"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_600_radius_17_5"
                            android:id="@+id/viewEllipseEleven"
                            android:layout_height="@dimen/_35pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_113pxv" android:layout_width="@dimen/_35pxh"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnellipsenine"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_75pxv"
                        android:layout_weight="0.45"
                        android:layout_width="0dp" android:orientation="vertical">
                        <View
                            android:background="@drawable/rectangle_bg_gray_600_radius_37_5"
                            android:id="@+id/viewEllipseNine"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_75pxh"
                            android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_75pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_600_radius_48"
                            android:id="@+id/viewEllipseTen"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_96pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_70pxv" android:layout_width="@dimen/_96pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
