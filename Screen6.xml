<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="menuVM" type="com.salarsapplication.app.modules.menu.data.viewmodel.MenuVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearMenu"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_500_b0">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackscreenshot2023"
            android:layout_height="@dimen/_612pxv"
            android:layout_marginBottom="@dimen/_20pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageScreenshot2023"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_77pxh"
                android:layout_marginBottom="@dimen/_93pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_93pxv"
                android:layout_width="@dimen/_80pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_screenshot2023_77x80}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_screenshot2023_77x80"/>
            <LinearLayout android:id="@+id/linearColumnvector"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="start"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowvector"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylegray_800">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_21pxv"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_gray_501"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_501"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSettings"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginStart="@dimen/_83pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_weight="0.7"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{menuVM.menuModel.txtSettings}"
                            style="@style/txtManjariregular24" tools:text="@string/lbl_settings"/>
                        <ImageView android:id="@+id/imageSearch"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_marginEnd="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_78pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_search_gray_50"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_search_gray_50"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSalarKhan"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_103pxh"
                    android:layout_marginStart="@dimen/_103pxh"
                    android:layout_marginTop="@dimen/_52pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{menuVM.menuModel.txtSalarKhan}"
                    style="@style/txtManjariregular20" tools:text="@string/lbl_salar_khan"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowvectorOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_79pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVectorOne"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_gray_50_25x22"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_50_25x22"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEditProfile"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.81"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuVM.menuModel.txtEditProfile}"
                        style="@style/txtManjariregular20" tools:text="@string/lbl_edit_profile"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowgroup"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_47pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageGroup"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtJoinCommunity"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.89"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuVM.menuModel.txtJoinCommunity}"
                        style="@style/txtManjariregular20" tools:text="@string/lbl_join_community"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStackgroupOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_247pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_53pxv" android:layout_width="@dimen/_198pxh">
                    <ImageView android:id="@+id/imageGroupOne"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_72pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_72pxv"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group_gray_50"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group_gray_50"/>
                    <ImageView android:id="@+id/imageIconReport"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_marginBottom="@dimen/_76pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_76pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconreport"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconreport"/>
                    <LinearLayout android:id="@+id/linearRowvectorTwo"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVectorTwo"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_marginBottom="@dimen/_223pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_gray_50_24x20"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_50_24x20"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnhelp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_37pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.87"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtHelp"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{menuVM.menuModel.txtHelp}"
                                style="@style/txtManjariregular20" tools:text="@string/lbl_help"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtAddFriend"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_51pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{menuVM.menuModel.txtAddFriend}"
                                style="@style/txtManjariregular20" tools:text="@string/lbl_add_friend"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtReportUser"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_54pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{menuVM.menuModel.txtReportUser}"
                                style="@style/txtManjariregular20" tools:text="@string/lbl_report_user"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtChangeTheme"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_53pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{menuVM.menuModel.txtChangeTheme}"
                                style="@style/txtManjariregular20" tools:text="@string/lbl_change_theme"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageVectorThree"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_gray_50_28x22"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_50_28x22"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
